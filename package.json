{
  "name": "API-TEST-TS",
  "version": "1.0.0",
  "description": "Playwright API Test",
  "main": "index.js",
  "scripts": {
    "pretest": "npx ts-node src/test/helper/report/init.ts",
    "posttest": "npx ts-node src/test/helper/report/report.ts",
    "run:test": "npx cucumber-js --tags \"$TAGS\" --config=config/cucumber.js",
    "test": "npm run pretest; npm run run:test; status=$?; npm run posttest; exit $status"
  },
  "keywords": [],
  "author": "Ayu",
  "dependencies": {
    "@cucumber/cucumber": "^10.3.2",
    "@playwright/test": "^1.35.1",
    "ajv": "^8.17.1",
    "dotenv": "^16.3.1",
    "jsonpath": "^1.1.1",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "path": "^0.12.7",
    "ts-node": "^10.9.1"
  }
}
